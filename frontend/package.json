    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "classnames": "^2.5.1",
    "framer-motion": "^12.23.24",
    "motion": "^12.23.24",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.10.0",
    "tailwind-merge": "^3.3.1"
